<p id="notice"><%= notice %></p>

<h2><%= @place.name %></h2>
<p><%= @place.street %></p>
<p><i><%= @place.zip %>, <%= @place.city %></i></p>

<iframe
  width="600"
  height="450"
  frameborder="0" style="border:0"
  src="https://www.google.com/maps/embed/v1/place?key=<%= @apikey %>&q=<%= @address %>" allowfullscreen>
</iframe>
